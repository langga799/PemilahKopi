<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:visibility="visible"    android:background="@color/white">    <TextView        android:id="@+id/item_id"        android:layout_width="50dp"        android:layout_height="50dp"        android:gravity="center"        android:paddingVertical="16dp"        android:text="1"        android:textColor="@color/black"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/tv_jenis_kopi"        android:layout_width="100dp"        android:layout_height="wrap_content"        android:paddingVertical="16dp"        android:text="Kopi Hijau"        android:textAlignment="textStart"        android:textColor="@color/black"        android:textSize="12sp"        app:layout_constraintBottom_toBottomOf="@+id/item_id"        app:layout_constraintStart_toEndOf="@+id/item_id"        app:layout_constraintTop_toTopOf="@+id/item_id" />    <TextView        android:id="@+id/tv_berat_kopi"        android:layout_width="100dp"        android:layout_height="wrap_content"        android:paddingVertical="16dp"        android:text="200"        android:textColor="@color/black"        android:textSize="12sp"        app:layout_constraintBottom_toBottomOf="@+id/tv_jenis_kopi"        app:layout_constraintStart_toEndOf="@+id/tv_jenis_kopi"        app:layout_constraintTop_toTopOf="@+id/tv_jenis_kopi" />    <TextView        android:id="@+id/tv_waktu"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginEnd="8dp"        android:paddingVertical="16dp"        android:text="19-08-2022 14:08:01"        android:textColor="@color/black"        android:textSize="12sp"        app:layout_constraintBottom_toBottomOf="@+id/tv_berat_kopi"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toEndOf="@+id/tv_berat_kopi"        app:layout_constraintTop_toTopOf="@+id/tv_berat_kopi" /></androidx.constraintlayout.widget.ConstraintLayout>